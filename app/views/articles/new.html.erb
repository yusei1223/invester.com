<div class="container m-5">
  <div class="row">
    <div class="col-6"></div>
    


<%= form_with model: @article, url:'/articles', local:true do |f| %>
  <h1>新規投稿</h1>
  <h4>イメージ画像</h4>
  <%= f.attachment_field :image %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <br>
  <%= f.submit "       投稿する      ", class:'btn btn-primary'%>
<% end %>

  </div>
</div>